#include<bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef double dl;

#define endl "\n"
#define mem(a,b) memset(a, b, sizeof(a))
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define fraction() cout.unsetf(ios::floatfield); cout.precision(10); cout.setf(ios::fixed,ios::floatfield);

int main()
{
    optimize();
    int t;
    cin >> t;
    while (t--){
        int n;
        cin>>n;
        map<int,int>cnt;
        for(int i=0;i<n;i++) {
            int x;
            cin>>x;
            cnt[x]++;
        }
        int ans=0;
        for(auto u:cnt){
            ans+=(u.second/2);
        }
        cout<<ans<<endl;
        
    }
    return 0;
}
